#= depend_on_asset "search.html"

@movieSearch = angular.module('movieSearch', ['ngRoute', 'movieSearch.controllers'])

@movieSearch.constant("MOVIE_API", "http://www.omdbapi.com/?")

@movieSearch.config(['$routeProvider', ($routeProvider)->
  $routeProvider.
    otherwise({
      templateUrl: '<%= asset_path("search.html") %>',
      controller: 'SearchCtrl'
    })
])